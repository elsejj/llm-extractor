{
  "manifest_version": 3,
  "name": "网页数据结构化",
  "description": "将网页中的内容转换成结构化数据",
  "version": "0.1.5",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "48": "logo_48x48.png",
      "128": "logo_128x128.png",
      "256": "logo_256x256.png"
    }
  },
  "icons": {
    "48": "logo_48x48.png",
    "128": "logo_128x128.png",
    "256": "logo_256x256.png"
  },
  "permissions": [
    "sidePanel",
    "tabs",
    "storage",
    "unlimitedStorage",
    "clipboardWrite"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background/background.js"
  },
  "content_scripts": [
    {
      "js": [
        "content_scripts/content_scripts.js"
      ],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  },
  "web_accessible_resources": [
    {
        "matches": [
            "<all_urls>"
        ],
        "resources": [
            "j2ew_bg.wasm"
        ]
    }
]
}